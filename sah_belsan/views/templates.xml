                <!--                            <li class="list-inline-item headr_login_list">-->
                <!--                                <t t-foreach="languages" t-as="lg">-->
                <!--                                    <a t-att-href="url_for(request.httprequest.path + '?' + keep_query(), lang=lg[0])" class="dropdown-item js_change_lang" t-att-data-url_code="lg[0]">-->
                <!--                                        <t t-esc="lg[1].split('/').pop()"/>-->
                <!--                                    </a>-->
                <!--                                    <span class="belsan_lang_border"></span>-->
                <!--                                </t>-->
                <!--                            </li>-->
                <!--                        </ul>-->
                <!--                    </p>-->
                <!--                </div>-->

                <div class="belsan_nav_search_container belsan_nav_search_open">
                    <div class="belsan_nav_search">
                        <form action="/shop" method="GET">
                            <div class="input-group belsan_header_search_bar">
                                <i class="fa fa-search belsan_header_search_icon" onclick="searchFun()"></i>
                                <div id="belsan_header_search_input">
                                    <input type="text" class="search-query form-control oe_search_box" name="search" aria-label="Search" placeholder="عن ماذا تبحث .." required="required" />
                                    <button type="submit" class="btn btn-link oe_search_button belsan_btn_search_icon">
                                        ابحث
                                    </button>
                                </div>
